<div class="json-editor-container">
  <div class="btn-toolbar" role="toolbar" style="padding-bottom: 1rem">
    <button type="button" disabled class="save-json btn btn-primary pull-xs-right"
      title="Save Changes to Registry/File System">Save</button>
    <button type="button" disabled class="reset-json btn btn-danger pull-xs-right"
      title="Reset to Original Schema">Reset JSON</button>
  </div>
  <pre id="edit-{filename|id}" class="json-editor"></pre>
</div>
